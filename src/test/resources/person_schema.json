{
  "type" : "object",
  "properties" : {
    "first_name" : {
      "type" : "string" ,
      "minLength"  : 3 ,
      "maxLength" : 255
    },
    "last_name" : {
      "type" : "string" ,
      "minLength"  : 3 ,
      "maxLength" : 255
    },
    "age"       : {
      "type" : "integer" ,
      "minimum" : 16 ,
      "maximum" : 40
    },
    "phone_number" : {
      "type" : "integer",
      "pattern" : "[6-9][0-9]{9}"
    } ,
    "email"     : {
      "type" : "string",
      "pattern" : "[a-z0-9]+"
    } ,
    "password" : {
      "type" : "string" ,
      "minLength" : 7 ,
      "maxLength" : 255 ,
      "pattern" : "^.{7, 255}$"
    } ,
    "gender" : { "enum" : ["Male" , "Female"] }
  },

  "required" : ["first_name","last_name" , "age"  ,"email" , "password"
  , "gender" ]
}